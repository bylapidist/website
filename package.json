{
  "name": "@lapidist/website",
  "version": "0.0.0",
  "description": "The Lapidist website living at lapidist.net",
  "author": "Brett Dorrans <hello@lapidist.net>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bylapidist/website.git"
  },
  "bugs": {
    "url": "https://github.com/bylapidist/website/issues"
  },
  "homepage": "https://github.com/bylapidist/website#readme",
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build": "npm run build:cv && npm run build:gatsby",
    "build:gatsby": "gatsby build --prefix-paths",
    "build:cv": "node ./node_modules/@lapidist/cv-generator/dist/es5/cli.js && mv brett-dorrans-cv.pdf static/",
    "lint": "eslint . --ext js,ts,tsx",
    "fix": "eslint --fix . --ext js,ts,tsx"
  },
  "dependencies": {
    "@fontsource/albert-sans": "^5.0.8",
    "@fontsource/montserrat": "^5.0.8",
    "@lapidist/components": "9.5.0",
    "@lapidist/cv-generator": "^1.7.4",
    "@mdx-js/react": "^2.3.0",
    "classnames": "^2.3.2",
    "gatsby": "^5.11.0",
    "gatsby-plugin-canonical-urls": "^5.11.0",
    "gatsby-plugin-google-gtag": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-mdx": "^5.11.0",
    "gatsby-plugin-offline": "^6.11.0",
    "gatsby-plugin-sass": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-source-graphql": "^5.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.64.2"
  },
  "devDependencies": {
    "@lapidist/linting": "3.10.9",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.1.6"
  }
}
