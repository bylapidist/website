{
    "name": "@lapidist/website",
    "version": "0.0.0",
    "description": "The Lapidist website living at lapidist.net",
    "author": "Brett Dorrans <hello@lapidist.net>",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bylapidist/website.git"
    },
    "bugs": {
        "url": "https://github.com/bylapidist/website/issues"
    },
    "homepage": "https://github.com/bylapidist/website#readme",
    "scripts": {
        "start": "gatsby develop --port 3000",
        "serve": "gatsby serve",
        "build": "npm run build:cv && npm run build:gatsby",
        "build:gatsby": "gatsby build --prefix-paths",
        "build:cv": "node ./node_modules/@lapidist/cv-generator/dist/es5 && mv brett-dorrans-cv.pdf static/",
        "clean": "gatsby clean",
        "lint": "eslint . --ext js,ts,tsx",
        "fix": "eslint --fix . --ext js,ts,tsx"
    },
    "devDependencies": {
        "@lapidist/cv-generator": "1.6.11",
        "@lapidist/linting": "3.3.1",
        "@types/node": "18.15.5",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/react-helmet": "6.1.6",
        "@types/styled-components": "5.1.26",
        "babel-plugin-styled-components": "2.0.7",
        "browserslist": "4.21.5",
        "typescript": "4.9.5"
    },
    "dependencies": {
        "@lapidist/components": "8.5.2",
        "dayjs": "1.11.7",
        "dotenv": "16.0.3",
        "gatsby": "5.7.0",
        "gatsby-image": "3.11.0",
