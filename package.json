        "@types/react-dom": "18.0.5",
        "@types/react-helmet": "6.1.5",
        "@types/styled-components": "5.1.25",
        "babel-plugin-styled-components": "2.0.7",
        "browserslist": "4.20.3",
        "typescript": "4.7.3"
    },
    "dependencies": {
        "@lapidist/components": "6.13.2",
        "dayjs": "1.11.2",
        "dotenv": "16.0.1",
        "gatsby": "4.15.2",
