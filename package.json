{
    "name": "@lapidist/website",
    "version": "0.0.0",
    "description": "The Lapidist website living at lapidist.net",
    "author": "Brett Dorrans <hello@lapidist.net>",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bylapidist/website.git"
    },
    "bugs": {
        "url": "https://github.com/bylapidist/website/issues"
    },
    "homepage": "https://github.com/bylapidist/website#readme",
    "scripts": {
        "start": "gatsby develop --port 3000",
        "serve": "gatsby serve",
        "build": "npm run build:cv && npm run build:gatsby",
        "build:gatsby": "gatsby build --prefix-paths",
        "build:cv": "node ./node_modules/@lapidist/cv-generator/dist/es5 && mv brett-dorrans-cv.pdf static/",
        "clean": "gatsby clean",
        "lint": "eslint . --ext js,ts,tsx",
        "fix": "eslint --fix . --ext js,ts,tsx"
    },
    "devDependencies": {
        "@lapidist/cv-generator": "1.2.1",
        "@lapidist/linting": "2.125.1",
        "@types/node": "16.11.25",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@types/react-helmet": "6.1.5",
        "@types/styled-components": "5.1.22",
        "babel-plugin-styled-components": "2.0.2",
        "browserslist": "4.19.1",
        "typescript": "4.5.5"
    },
    "dependencies": {
        "@lapidist/components": "6.6.4",
        "gatsby": "4.7.1",
        "gatsby-image": "3.11.0",
        "gatsby-plugin-canonical-urls": "4.7.0",
        "gatsby-plugin-google-analytics": "4.7.0",
        "gatsby-plugin-manifest": "4.7.0",
        "gatsby-plugin-offline": "5.7.0",
        "gatsby-plugin-react-helmet": "5.7.0",
        "gatsby-plugin-sharp": "4.7.0",
        "gatsby-plugin-styled-components": "5.7.0",
        "gatsby-plugin-typescript": "4.7.0",
        "gatsby-remark-copy-linked-files": "5.7.0",
        "gatsby-remark-images": "6.7.0",
        "gatsby-remark-prismjs": "6.7.0",
        "gatsby-remark-responsive-iframe": "5.7.0",
        "gatsby-remark-smartypants": "5.7.0",
        "gatsby-source-filesystem": "4.7.0",
        "gatsby-transformer-json": "4.7.0",
        "gatsby-transformer-remark": "5.7.0",
        "gatsby-transformer-sharp": "4.7.0",
        "postcss": "8.4.6",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-loader": "6.2.1",
        "prism-themes": "1.9.0",
        "prismjs": "1.26.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "styled-components": "5.3.3"
    }
}
