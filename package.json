    "@lapidist/cv-generator": "2.4.0",
    "classnames": "2.5.1",
    "gatsby": "5.14.3",
    "gatsby-plugin-canonical-urls": "5.14.0",
    "gatsby-plugin-google-gtag": "5.14.0",
